<!DOCTYPE html>
<!--[if IE 8]><html class="ie8 no-js" lang="en"><![endif]-->
<!--[if IE 9]><html class="ie9 no-js" lang="en"><![endif]-->
<!--[if !IE]><!-->
<html lang="en" class="no-js">
<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

    <!-- SITE META -->
    <title>{% block title %}ListeDeCourse{% endblock %}</title>
    <meta name="description" content="">
    <meta name="author" content="">
    <meta name="keywords" content="">

    <!-- FAVICONS -->
    <link rel="shortcut icon" href="{{ asset("front-assets/images/favicon.ico" )}}">
    {% block stylesheets %}
    <!-- BOOTSTRAP STYLES -->
    <link rel="stylesheet" href="{{ asset("front-assets/css/bootstrap.min.css") }}">
    <!-- TEMPLATE STYLES -->
    <link rel="stylesheet" href="{{ asset("front-assets/css/style.css") }}">
    <!-- RESPONSIVE STYLES -->
    <link rel="stylesheet" href="{{ asset("front-assets/css/responsive.css")}}">
    <!-- COLORS -->
    <link rel="stylesheet" href="{{ asset("front-assets/css/colors.css")}}">
    <!-- CUSTOM STYLES -->
    <link rel="stylesheet" href="{{ asset("front-assets/css/custom.css")}}">


    <!-- Skin Examples -->
    <link rel="stylesheet" href="{{ asset("front-assets/css/skins/skin1.css" )}}">
    <link rel="stylesheet" href="{{ asset("front-assets/css/skins/skin2.css")}}">
    <link rel="stylesheet" href="{{ asset("front-assets/css/skins/skin3.css") }}">
    <link rel="stylesheet" href="{{ asset("front-assets/css/skins/skin4.css") }}">
    <link rel="stylesheet" href="{{ asset("front-assets/css/skins/skin5.css") }}">
