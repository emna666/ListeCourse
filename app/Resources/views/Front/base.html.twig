       {% block contentTop %}{% endblock %}
    </div><!-- end section -->

    <div class="section wb">
        {% block body %}{% endblock %}
    </div><!-- end section -->
    {% include ':Front:footer.html.twig' %}

</div><!-- end wrapper -->

{% block javascripts %}
<script src={{ asset("front-assets/js/all.js")}}></script>
<script src={{ asset("front-assets/js/custom.js")}}></script>
<script src={{ asset("switcher/switcher.js")}}></script>
{% endblock %}
